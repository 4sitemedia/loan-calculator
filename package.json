{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write \"**/*.{js,ts,tsx,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint .",
    "pre-commit": "npm run type-check && npm run lint",
